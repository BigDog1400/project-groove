{
  "expo": {
    "name": "Grease the Groove",
    "slug": "grease-the-groove",
    "scheme": "grease-the-groove",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#000000",
          "resizeMode": "cover",
          "image": "./assets/splash-dark.png"
        }
      },
      "bundleIdentifier": "com.greasethegroovetraining.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#000000",
          "resizeMode": "cover",
          "image": "./assets/splash-dark.png"
        }
      },
      "package": "com.greasethegroovetraining.app"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ]
    ],
    "description": "Master your strength with simple, consistent Grease the Groove practice.",
    "subtitle": "Daily Training",
    "extra": {
      "eas": {
        "projectId": "637b247b-7862-4c2b-bd8f-abb29de54f18"
      }
    }
  }
}
